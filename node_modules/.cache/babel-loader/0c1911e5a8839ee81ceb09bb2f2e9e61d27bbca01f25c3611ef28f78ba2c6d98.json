{"ast":null,"code":"import { openProcedure } from \"@/utils/monitoringApp\";\nimport { eventBus } from '@/utils/eventBus';\nimport Abnormal from \"./abnormal\";\nexport default {\n  data() {\n    return {\n      step: 1,\n      abnormalStatus: 0,\n      //驱动启动值 0启动中 1未安装 2启动失败 3更新 4已安装\n      openSocket: true //是否连接socket\n    };\n  },\n  components: {\n    Abnormal\n  },\n  created() {\n    eventBus.$on('socketStatusChange', val => {\n      if (val == 3) {\n        //连接失败\n        this.abnormalStatus = 2;\n      }\n    });\n  },\n  mounted() {\n    let self = this;\n    eventBus.$on('socketMessage', data => {\n      console.log(data);\n      switch (data.path) {\n        case 'getAppInfo':\n          //  判断是否有更新，没有更新进行下一步\n          self.step = 3;\n      }\n    });\n  },\n  methods: {\n    firing() {\n      let self = this;\n      this.abnormalStatus = 0;\n      let appUri = 'CGM3Uploader://'; // 应用程序的地址(注册表应用名称)\n      // let appUri = 'Clashx://'; // 应用程序的地址(注册表应用名称)\n      let timeoutDuration = 1000;\n      let openAppFailure = () => {\n        //未检测到驱动，需要下载\n        self.abnormalStatus = 1;\n      };\n      let openAppSuccess = () => {\n        // 已安装驱动\n        self.abnormalStatus = 4;\n        self.step = 2;\n        setTimeout(function () {\n          //等待用户点击弹窗\n          self.$websocket.reset();\n          self.$websocket.initwebSocket();\n          self.abnormalStatus = 0;\n        }, 3000);\n      };\n      openProcedure(appUri, openAppFailure, openAppSuccess, timeoutDuration);\n    },\n    // 下载\n    downLoad() {\n      console.log('下载驱动');\n      this.abnormalStatus = 0;\n      this.step = 1;\n      // window.location.href='https://dldir1.qq.com/weixin/mac/WeChatMac.dmg'\n    }\n  }\n};","map":{"version":3,"names":["openProcedure","eventBus","Abnormal","data","step","abnormalStatus","openSocket","components","created","$on","val","mounted","self","console","log","path","methods","firing","appUri","timeoutDuration","openAppFailure","openAppSuccess","setTimeout","$websocket","reset","initwebSocket","downLoad"],"sources":["src/views/Procedure/upLoad.vue"],"sourcesContent":["<template>\n    <div class='upLoad' >\n        <div class='run-procedure'  v-if='abnormalStatus==0||abnormalStatus==4'>\n            <div class='step-box step1' v-if='step==1'>\n                 <div class='title' >运行驱动程序</div>\n                <div class='procedure_img' >\n                    <div class='procedure_img_item' >1</div>\n                    <div class='procedure_img_item' >2</div>\n                    <div class='procedure_img_item' >3</div>\n                    <div class='procedure_img_item' >4</div>\n                </div>\n                <div class='btn-box' >\n                    <el-button type=\"primary\"  @click='downLoad'> 下载驱动</el-button>\n                    <el-button type=\"primary\"  @click='firing'>启动驱动</el-button>\n                </div>\n            </div>\n           <div class='step-box step2' v-if='step==2' >\n                <i class=\"el-icon-loading loading-icon \"></i>\n                <div class='unistall-text' >正在启动中......</div>\n           </div>\n           <div class='step-box step1' v-if='step==3'>\n                 <div class='title' >连接读取器</div>\n                <div class='procedure_img' >\n                    <div class='procedure_img_item' >1</div>\n                    <div class='procedure_img_item' >2</div>\n                </div>\n                <div class='btn-box flex-end' >\n                    <el-button type=\"primary\"  @click='downLoad'> 开始连接</el-button>\n                </div>\n            </div>\n        </div>\n        <Abnormal v-if='abnormalStatus!=0' :type='abnormalStatus' @downLoad='downLoad' @firing='firing' />\n    </div>\n</template>\n<script>\nimport { openProcedure } from \"@/utils/monitoringApp\";\nimport {eventBus} from '@/utils/eventBus'\nimport Abnormal from \"./abnormal\"\nexport default {\n    data(){\n        return{\n            step:1,\n            abnormalStatus:0, //驱动启动值 0启动中 1未安装 2启动失败 3更新 4已安装\n            openSocket:true, //是否连接socket\n        }\n    },\n    components:{\n        Abnormal\n    },\n    created(){\n        eventBus.$on('socketStatusChange',(val) => {\n            if(val==3){ //连接失败\n                this.abnormalStatus =  2\n            }\n        })\n    }, \n    mounted(){\n        let self = this\n        eventBus.$on('socketMessage',(data) => {\n            console.log(data)\n            switch(data.path){\n                case 'getAppInfo':\n                    //  判断是否有更新，没有更新进行下一步\n                    self.step = 3\n            }\n        })\n    },\n    methods:{\n        firing(){\n            let self = this\n            this.abnormalStatus = 0\n            let appUri = 'CGM3Uploader://'; // 应用程序的地址(注册表应用名称)\n            // let appUri = 'Clashx://'; // 应用程序的地址(注册表应用名称)\n            let timeoutDuration = 1000;\n            let openAppFailure = () => { //未检测到驱动，需要下载\n                self.abnormalStatus = 1\n            };\n            let openAppSuccess = () => {\n                // 已安装驱动\n                self.abnormalStatus = 4\n                self.step = 2\n                setTimeout(function(){ //等待用户点击弹窗\n                    self.$websocket.reset();\n                    self.$websocket.initwebSocket();\n                    self.abnormalStatus = 0\n                },3000)\n\n            };\n            openProcedure(appUri, openAppFailure, openAppSuccess, timeoutDuration);\n        },\n        // 下载\n        downLoad(){\n            console.log('下载驱动')\n            this.abnormalStatus = 0\n            this.step = 1\n            // window.location.href='https://dldir1.qq.com/weixin/mac/WeChatMac.dmg'\n        }\n    }\n}\n</script>\n<style scoped>\n    .upLoad{\n        width:100%;\n        height:100%;\n        padding:30px 60px;\n        background:#fff;\n    }\n    .run-procedure{\n         height:100%;\n    }\n    /* 步骤一 */\n    .step1{\n        position: relative;\n        height:100%;\n        width:100%;\n        display: flex;\n        flex-direction: column;\n        justify-content: space-between;\n    }\n     .title{\n        font-size:24px;\n        color:#333;\n        font-weight: 700;\n        margin-bottom:20px;\n    }\n    .procedure_img{\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n    }\n    .procedure_img_item{\n        flex-grow: 1;\n        height:460px;\n        margin:0 10px;\n        background:#d9d9d9;\n        text-align: center;\n        line-height: 460px;\n    }\n    .btn-box{\n        display: flex;\n        align-items: center;\n        justify-content:space-between;\n        \n    }\n    .btn-box.flex-end{\n        justify-content: flex-end;\n    }\n    /* 步骤2 */\n     .step2{\n       display: flex;\n       align-items: center;\n       justify-content: center; \n       flex-direction: column;\n        height:100%;\n    }\n    .loading-icon{\n        font-size:60px;\n        color:var(--color-primary);\n    }\n    /* 未安装 */\n    .uninstall-procedure{\n        height:100%;\n        position: relative;\n    }\n     .unstall-tips{\n        display:flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n        height:100%;\n    }\n    .waring-icon{\n        font-size:60px;\n        color:red;\n    }\n    .unistall-text{\n        margin-top:30px;\n        font-size:18px;\n        text-align: center;\n    }\n    .unistall-btn{\n        display: flex;\n        justify-content: flex-end;\n        position: absolute;\n        right:0;\n        bottom:0;\n    }\n   \n  \n</style>"],"mappings":"AAmCA,SAAAA,aAAA;AACA,SAAAC,QAAA;AACA,OAAAC,QAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,cAAA;MAAA;MACAC,UAAA;IACA;EACA;EACAC,UAAA;IACAL;EACA;EACAM,QAAA;IACAP,QAAA,CAAAQ,GAAA,uBAAAC,GAAA;MACA,IAAAA,GAAA;QAAA;QACA,KAAAL,cAAA;MACA;IACA;EACA;EACAM,QAAA;IACA,IAAAC,IAAA;IACAX,QAAA,CAAAQ,GAAA,kBAAAN,IAAA;MACAU,OAAA,CAAAC,GAAA,CAAAX,IAAA;MACA,QAAAA,IAAA,CAAAY,IAAA;QACA;UACA;UACAH,IAAA,CAAAR,IAAA;MACA;IACA;EACA;EACAY,OAAA;IACAC,OAAA;MACA,IAAAL,IAAA;MACA,KAAAP,cAAA;MACA,IAAAa,MAAA;MACA;MACA,IAAAC,eAAA;MACA,IAAAC,cAAA,GAAAA,CAAA;QAAA;QACAR,IAAA,CAAAP,cAAA;MACA;MACA,IAAAgB,cAAA,GAAAA,CAAA;QACA;QACAT,IAAA,CAAAP,cAAA;QACAO,IAAA,CAAAR,IAAA;QACAkB,UAAA;UAAA;UACAV,IAAA,CAAAW,UAAA,CAAAC,KAAA;UACAZ,IAAA,CAAAW,UAAA,CAAAE,aAAA;UACAb,IAAA,CAAAP,cAAA;QACA;MAEA;MACAL,aAAA,CAAAkB,MAAA,EAAAE,cAAA,EAAAC,cAAA,EAAAF,eAAA;IACA;IACA;IACAO,SAAA;MACAb,OAAA,CAAAC,GAAA;MACA,KAAAT,cAAA;MACA,KAAAD,IAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}