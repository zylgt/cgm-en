{"ast":null,"code":"import ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  components: {\n    ScaleBox\n  },\n  data() {\n    return {\n      height: 1080\n    };\n  },\n  created() {\n    this.height = window.innerHeight;\n    let screenWidth = window.screen.width;\n    console.log(window.innerWidth, window.screen.width);\n    let uiWidth = 1920;\n    let zoom = Math.round(screenWidth / 1920);\n    console.log(zoom);\n  },\n  mounted() {\n    // 监听浏览器刷新关闭\n\n    console.log();\n    new devPixelRatio().init(); // 初始化页面比例\n    window.addEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  },\n  methods: {\n    beforeunloadHandler(e) {\n      if (this.$store.getters.upStep == 4) {\n        e = e || window.event;\n        if (e) {\n          e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n    }\n  },\n  destroyed() {\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n};","map":{"version":3,"names":["ScaleBox","devPixelRatio","name","components","data","height","created","window","innerHeight","screenWidth","screen","width","console","log","innerWidth","uiWidth","zoom","Math","round","mounted","init","addEventListener","e","beforeunloadHandler","methods","$store","getters","upStep","event","returnValue","destroyed","removeEventListener"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n      <!-- <scale-box :width=\"1920\" :height='height' bgc=\"transparent\" :delay=\"100\">\n       \n      </scale-box> -->\n     <router-view />\n  </div>\n</template>\n\n<script>\nimport ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  components: { ScaleBox },\n  data(){\n    return{\n      height:1080\n    }\n  },\n  created(){\n    this.height = window.innerHeight\n    let screenWidth = window.screen.width;\n    console.log(window.innerWidth, window.screen.width)\n    let uiWidth =1920\n    let zoom = Math.round(screenWidth/1920)\n    console.log(zoom)\n  },\n  mounted(){\n    // 监听浏览器刷新关闭\n\n    console.log()\n    new devPixelRatio().init(); // 初始化页面比例\n    window.addEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  },\n  methods: {\n    beforeunloadHandler(e){\n      if(this.$store.getters.upStep == 4){\n       e = e || window.event;\n        if (e) {\n            e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n     \n    }\n  },\n  destroyed(){\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n}\n</script>\n\n<style>\nhtml,body{\n  width:1920px !important;\n  height:1080px !important;\n  padding:0;\n  margin:0;\n}\n#app {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  width:100%;\n  height:100%;\n\n}\n.vue2-scale-box{\n  left:0 !important;\n  top:0 !important;\n  transform: translate(0) !important;\n}\n</style>\n"],"mappings":"AAUA,OAAAA,QAAA;AACA,OAAAC,aAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,KAAA;IACA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAD,MAAA,GAAAE,MAAA,CAAAC,WAAA;IACA,IAAAC,WAAA,GAAAF,MAAA,CAAAG,MAAA,CAAAC,KAAA;IACAC,OAAA,CAAAC,GAAA,CAAAN,MAAA,CAAAO,UAAA,EAAAP,MAAA,CAAAG,MAAA,CAAAC,KAAA;IACA,IAAAI,OAAA;IACA,IAAAC,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAT,WAAA;IACAG,OAAA,CAAAC,GAAA,CAAAG,IAAA;EACA;EACAG,QAAA;IACA;;IAEAP,OAAA,CAAAC,GAAA;IACA,IAAAZ,aAAA,GAAAmB,IAAA;IACAb,MAAA,CAAAc,gBAAA,iBAAAC,CAAA,SAAAC,mBAAA,CAAAD,CAAA;EACA;EACAE,OAAA;IACAD,oBAAAD,CAAA;MACA,SAAAG,MAAA,CAAAC,OAAA,CAAAC,MAAA;QACAL,CAAA,GAAAA,CAAA,IAAAf,MAAA,CAAAqB,KAAA;QACA,IAAAN,CAAA;UACAA,CAAA,CAAAO,WAAA;QACA;QACA;MACA;IAEA;EACA;EACAC,UAAA;IACAvB,MAAA,CAAAwB,mBAAA,iBAAAT,CAAA,SAAAC,mBAAA,CAAAD,CAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}