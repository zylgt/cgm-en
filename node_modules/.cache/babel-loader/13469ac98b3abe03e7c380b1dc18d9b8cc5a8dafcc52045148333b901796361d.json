arW,value)\n                start_y = start_y + value + splitInterval\n                ctx.lineWidth=\"1\";\n                ctx.strokeStyle=\"#e5e5e5\";\n                ctx.moveTo(lineStart,start_y-(value/2));\n                ctx.lineTo(lineStart+lineWidth+i*10,start_y-(value/2));\n                ctx.lineTo(lineStart+lineWidth+i*10,start_y-(value/2)+i*10);\n                ctx.lineTo(lineStart+lineWidth+i*10+30,start_y-(value/2)+i*20);\n                ctx.stroke();\n            }\n\n        }\n    }\n}\n</script>\n<style scoped>\n    .TIR{\n        height:280px;\n    }\n</style>"],"mappings":"AAMA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,MAAA,GACA;QAAAC,GAAA;QAAAC,KAAA;QAAAC,KAAA;QAAAC,IAAA;MAAA,GACA;QACAH,GAAA;QACAC,KAAA;QACAC,KAAA;QACAC,IAAA;MACA,GACA;QACAH,GAAA;QACAC,KAAA;QACAC,KAAA;QACAC,IAAA;MACA,GACA;QAAAH,GAAA;QAAAC,KAAA;QAAAC,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAH,GAAA;QAAAC,KAAA;QAAAC,KAAA;QAAAC,IAAA;MAAA;IAEA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,QAAA;MACA,MAAAE,MAAA,GAAAC,QAAA,CAAAC,cAAA;MACA,MAAAC,GAAA,GAAAH,MAAA,CAAAI,UAAA;MACA,MAAAC,CAAA,QAAAC,KAAA,CAAAC,GAAA,CAAAC,WAAA;MACA,MAAAC,CAAA,QAAAH,KAAA,CAAAC,GAAA,CAAAG,YAAA;MACAV,MAAA,CAAAW,KAAA,GAAAN,CAAA;MACAL,MAAA,CAAAY,MAAA,GAAAH,CAAA;MACA,MAAAI,IAAA;MACA,MAAAC,aAAA;MACA,MAAAC,SAAA,GAAAF,IAAA;MACA,MAAAG,SAAA;MACA,IAAAC,OAAA;;MAEA,SAAAC,CAAA,MAAAA,CAAA,QAAA1B,MAAA,CAAA2B,MAAA,EAAAD,CAAA;QACA,IAAAxB,KAAA,GAAA0B,MAAA,MAAA5B,MAAA,CAAA0B,CAAA,EAAAxB,KAAA;QACAS,GAAA,CAAAkB,SAAA,QAAA7B,MAAA,CAAA0B,CAAA,EAAAvB,KAAA;QACAQ,GAAA,CAAAmB,QAAA,IAAAL,OAAA,EAAAJ,IAAA,EAAAnB,KAAA;QACAuB,OAAA,GAAAA,OAAA,GAAAvB,KAAA,GAAAoB,aAAA;QACAX,GAAA,CAAAa,SAAA;QACAb,GAAA,CAAAoB,WAAA;QACApB,GAAA,CAAAqB,MAAA,CAAAT,SAAA,EAAAE,OAAA,GAAAvB,KAAA;QACAS,GAAA,CAAAsB,MAAA,CAAAV,SAAA,GAAAC,SAAA,GAAAE,CAAA,OAAAD,OAAA,GAAAvB,KAAA;QACAS,GAAA,CAAAsB,MAAA,CAAAV,SAAA,GAAAC,SAAA,GAAAE,CAAA,OAAAD,OAAA,GAAAvB,KAAA,OAAAwB,CAAA;QACAf,GAAA,CAAAsB,MAAA,CAAAV,SAAA,GAAAC,SAAA,GAAAE,CAAA,YAAAD,OAAA,GAAAvB,KAAA,OAAAwB,CAAA;QACAf,GAAA,CAAAuB,MAAA;MACA;IAEA