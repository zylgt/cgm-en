{"ast":null,"code":"import ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  components: {\n    ScaleBox\n  },\n  mounted() {\n    // 监听浏览器刷新关闭\n    new devPixelRatio().init(); // 初始化页面比例\n    window.addEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  },\n  methods: {\n    beforeunloadHandler(e) {\n      if (this.$store.getters.upStep == 4) {\n        e = e || window.event;\n        if (e) {\n          e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n    }\n  },\n  destroyed() {\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n};","map":{"version":3,"names":["ScaleBox","devPixelRatio","name","components","mounted","init","window","addEventListener","e","beforeunloadHandler","methods","$store","getters","upStep","event","returnValue","destroyed","removeEventListener"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n      <scale-box :width=\"1920\" bgc=\"transparent\" :delay=\"100\">\n        <router-view />\n      </scale-box>\n    \n  </div>\n</template>\n\n<script>\nimport ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  components: { ScaleBox },\n  mounted(){\n    // 监听浏览器刷新关闭\n    new devPixelRatio().init(); // 初始化页面比例\n    window.addEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  },\n  methods: {\n    beforeunloadHandler(e){\n      if(this.$store.getters.upStep == 4){\n       e = e || window.event;\n        if (e) {\n            e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n     \n    }\n  },\n  destroyed(){\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n}\n</script>\n\n<style>\nhtml,body{\n  width:1920px !important;\n  height:1080px !important;\n  padding:0;\n  margin:0;\n}\n#app {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  width:100%;\n  height:100%;\n\n}\n</style>\n"],"mappings":"AAUA,OAAAA,QAAA;AACA,OAAAC,aAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,QAAA;IACA;IACA,IAAAH,aAAA,GAAAI,IAAA;IACAC,MAAA,CAAAC,gBAAA,iBAAAC,CAAA,SAAAC,mBAAA,CAAAD,CAAA;EACA;EACAE,OAAA;IACAD,oBAAAD,CAAA;MACA,SAAAG,MAAA,CAAAC,OAAA,CAAAC,MAAA;QACAL,CAAA,GAAAA,CAAA,IAAAF,MAAA,CAAAQ,KAAA;QACA,IAAAN,CAAA;UACAA,CAAA,CAAAO,WAAA;QACA;QACA;MACA;IAEA;EACA;EACAC,UAAA;IACAV,MAAA,CAAAW,mBAAA,iBAAAT,CAAA,SAAAC,mBAAA,CAAAD,CAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}