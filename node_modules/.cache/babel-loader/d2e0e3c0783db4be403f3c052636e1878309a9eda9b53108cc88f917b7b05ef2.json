{"ast":null,"code":"import { TIRUtils } from \"@/utils/algorithm/TIR\";\nimport { formatDate, formatTime } from '@/utils/formatTime';\nexport default {\n  name: 'TIR',\n  data() {\n    return {\n      levels: [{\n        key: '2 级高血糖',\n        value: 1,\n        duration: [],\n        color: '#E98C41',\n        desc: '>13.9mmol/L',\n        target: '5',\n        index: 0\n      }, {\n        key: '1 级高血糖',\n        value: 1,\n        duration: [],\n        color: '#F6C059',\n        desc: '5.1-13.9mmol/L',\n        target: '25',\n        index: 1\n      }, {\n        key: '目标范围内',\n        value: 60,\n        duration: [],\n        color: '#32BAC0',\n        desc: '3.9~10.0mmol/L',\n        target: '70',\n        index: 2\n      }, {\n        key: '1 级低血糖',\n        value: 30,\n        duration: [],\n        color: '#F43F31',\n        desc: '3.0~3.8mmol/L',\n        target: '4',\n        index: 3\n      }, {\n        key: '2 级低血糖',\n        value: 0,\n        duration: [],\n        color: '#96251C',\n        desc: '<3.0mmol/L',\n        target: '1',\n        index: 4\n      }],\n      tirList: [],\n      canvas: null,\n      ctx: null,\n      highTarget: 0,\n      lowTarget: 0,\n      highTargetHeight: 0,\n      lowTargetHeight: 0\n    };\n  },\n  created() {},\n  mounted() {\n    console.log(formatTime(new Date()), '组件渲染完成');\n    this.$bus.$on('getDatas', data => {\n      let new_data = _.uniqBy(data[1], 'DataTs');\n      let v_data = _.map(new_data, 'Value');\n      let result = TIRUtils.getTIRResult(v_data);\n      console.log(result);\n      this.$store.dispatch('setTir', {\n        key: data[0],\n        value: result\n      }); //处理完数据存vuex\n      this.levels[0].value = _.round(Number(result.veryHighRate * 100), 1);\n      this.levels[1].value = _.round(Number(result.highRate * 100), 1);\n      this.levels[2].value = _.round(Number(result.normalRate * 100), 1);\n      this.levels[3].value = _.round(Number(result.lowRate * 100), 1);\n      this.levels[4].value = _.round(Number(result.veryLowRate * 100), 1);\n      this.levels[0].duration = [Math.floor(result.veryHighDuration / 60) < 10 ? '0' + Math.floor(result.veryHighDuration / 60) : Math.floor(result.veryHighDuration / 60), result.veryHighDuration % 60 < 10 ? '0' + result.veryHighDuration % 60 : result.veryHighDuration % 60];\n      this.levels[1].duration = [Math.floor(result.highDuration / 60) < 10 ? '0' + Math.floor(result.highDuration / 60) : Math.floor(result.highDuration / 60), result.highDuration % 60 < 10 ? '0' + result.highDuration % 60 : result.highDuration % 60];\n      this.levels[2].duration = [Math.floor(result.normalDuration / 60) < 10 ? '0' + Math.floor(result.normalDuration / 60) : Math.floor(result.normalDuration / 60), result.normalDuration % 60 < 10 ? '0' + result.normalDuration % 60 : result.normalDuration % 60];\n      this.levels[3].duration = [Math.floor(result.lowDuration / 60) < 10 ? '0' + Math.floor(result.lowDuration / 60) : Math.floor(result.lowDuration / 60), result.lowDuration % 60 < 10 ? '0' + result.lowDuration % 60 : result.lowDuration % 60];\n      this.levels[4].duration = [Math.floor(result.veryLowDuration / 60) < 10 ? '0' + Math.floor(result.veryLowDuration / 60) : Math.floor(result.veryLowDuration / 60), result.veryLowDuration % 60 < 10 ? '0' + result.veryLowDuration % 60 : result.veryLowDuration % 60];\n      this.highTarget = this.levels[0].value && this.levels[1].value ? _.round(this.levels[0].value + this.levels[1].value, 1) : 0;\n      this.lowTarget = this.levels[3].value && this.levels[4].value ? _.round(this.levels[4].value + this.levels[4].value, 1) : 0;\n      this.tirList = _.filter(this.levels, function (e) {\n        return e.value > 0;\n      });\n      this.$nextTick(() => {\n        this.drawTir();\n      });\n    });\n    this.$bus.$on('getVuexTag', data => {\n      let result = this.$store.state.report.tir[data];\n      console.log(result);\n      this.levels[0].value = _.round(Number(result.veryHighRate * 100), 1);\n      this.levels[1].value = _.round(Number(result.highRate * 100), 1);\n      this.levels[2].value = _.round(Number(result.normalRate * 100), 1);\n      this.levels[3].value = _.round(Number(result.lowRate * 100), 1);\n      this.levels[4].value = _.round(Number(result.veryLowRate * 100), 1);\n      console.log(result.veryHighDuration / 60, result.veryHighDuration % 60);\n      this.levels[0].duration = [Math.floor(result.veryHighDuration / 60) < 10 ? '0' + Math.floor(result.veryHighDuration / 60) : Math.floor(result.veryHighDuration / 60), result.veryHighDuration % 60 < 10 ? '0' + result.veryHighDuration % 60 : result.veryHighDuration % 60];\n      this.levels[1].duration = [Math.floor(result.highDuration / 60) < 10 ? '0' + Math.floor(result.highDuration / 60) : Math.floor(result.highDuration / 60), result.highDuration % 60 < 10 ? '0' + result.highDuration % 60 : result.highDuration % 60];\n      this.levels[2].duration = [Math.floor(result.normalDuration / 60) < 10 ? '0' + Math.floor(result.normalDuration / 60) : Math.floor(result.normalDuration / 60), result.normalDuration % 60 < 10 ? '0' + result.normalDuration % 60 : result.normalDuration % 60];\n      this.levels[3].duration = [Math.floor(result.lowDuration / 60) < 10 ? '0' + Math.floor(result.lowDuration / 60) : Math.floor(result.lowDuration / 60), result.lowDuration % 60 < 10 ? '0' + result.lowDuration % 60 : result.lowDuration % 60];\n      this.levels[4].duration = [Math.floor(result.veryLowDuration / 60) < 10 ? '0' + Math.floor(result.veryLowDuration / 60) : Math.floor(result.veryLowDuration / 60), result.veryLowDuration % 60 < 10 ? '0' + result.veryLowDuration % 60 : result.veryLowDuration % 60];\n      this.highTarget = this.levels[0].value && this.levels[1].value ? _.round(this.levels[0].value + this.levels[1].value, 1) : 0;\n      this.lowTarget = this.levels[3].value && this.levels[4].value ? _.round(this.levels[4].value + this.levels[4].value, 1) : 0;\n      this.tirList = _.filter(this.levels, function (e) {\n        return e.value > 0;\n      });\n      this.$nextTick(() => {\n        this.drawTir();\n        this.handelHeight();\n      });\n    });\n  },\n  methods: {\n    // 计算高血糖，低血糖总比例的高度\n    handelHeight() {\n      let legend = _.sortBy(this.$refs.legend, ['dataset.index']); //图例的位置\n      if (this.highTarget != 0) {\n        // 计算高度\n        this.highTargetHeight = legend[1].offsetTop - legend[0].offsetTop;\n      }\n      if (this.lowTarget != 0) {\n        this.lowTargetHeight = legend[4].offsetTop - legend[3].offsetTop;\n      }\n    },\n    // 画图\n    drawTir() {\n      const canvas = document.getElementById(\"myCanvas\");\n      const ctx = canvas.getContext(\"2d\");\n      const W = 160;\n      const H = this.$refs.tir.offsetHeight * 2;\n      canvas.width = W;\n      canvas.height = H;\n      ctx.scale(2, 2);\n      ctx.clearRect(0, 0, canvas.width, canvas.height);\n      const barW = 40; //柱状图宽度\n      const splitInterval = 1; //间隔距离\n      const lineStart = barW + 5; //标识线x轴起点\n      const lineWidth = 30; //标识线总宽度\n      let start_y = 2; //柱状图y轴起始点 \n      let start_text_y = this.$refs.tir.offsetTop; //第一段文字说明的起点\n      let legend = _.sortBy(this.$refs.legend, ['dataset.index']); //图例的位置\n      let noRangeLength = 1; //需要花第2段线的数量\n      for (var i = 0; i < this.tirList.length; i++) {\n        let value = Number(this.tirList[i].value) * 2.8;\n\n        // 画柱\n        ctx.fillStyle = this.tirList[i].color;\n        ctx.fillRect(0, start_y, barW, value);\n\n        // 画线\n        let tir_key_top = null;\n        if (legend[i]) tir_key_top = legend[i].offsetTop + 3;\n        let lineOneX = 0;\n        let lineOneY = 0;\n        let lineTwoX = 0;\n        let lineTwoY = 0;\n        let lineThreeX = 0;\n        let lineThreeY = 0;\n        if (tir_key_top - start_y > 10 && start_y + value - tir_key_top > 10) {\n          lineOneX = lineStart + lineWidth;\n          lineOneY = tir_key_top;\n          lineTwoX = lineOneX;\n          lineTwoY = lineOneY;\n          lineThreeX = lineOneX;\n          lineThreeY = lineOneY;\n          noRangeLength = 1;\n        } else {\n          noRangeLength++;\n          lineOneX = tir_key_top < start_y + value ? lineStart + lineWidth - (lineWidth - noRangeLength * 3) : lineStart + (lineWidth - noRangeLength * 3) - lineWidth / 2;\n          lineOneY = start_y + value / 2;\n          lineTwoX = tir_key_top < start_y + value ? lineStart + lineWidth - (lineWidth - noRangeLength * 3) : lineStart + (lineWidth - noRangeLength * 3) - lineWidth / 2;\n          lineTwoY = tir_key_top;\n          lineThreeX = lineStart + lineWidth;\n          lineThreeY = tir_key_top;\n        }\n        if (value != 0) {\n          ctx.lineWidth = \"1\";\n          ctx.strokeStyle = \"#e5e5e5\";\n          ctx.beginPath();\n          ctx.moveTo(lineStart, lineOneY);\n          ctx.lineTo(lineOneX, lineOneY);\n          ctx.lineTo(lineTwoX, lineTwoY);\n          ctx.lineTo(lineThreeX, lineThreeY);\n          ctx.stroke();\n        }\n        start_y = start_y + value + splitInterval;\n      }\n      this.$nextTick(() => {\n        console.log(formatTime(new Date()), '渲染完成');\n      });\n    }\n  },\n  beforeDestroy() {\n    this.$bus.$off('getDatas');\n    this.$bus.$off('getVuexTag');\n  }\n};","map":{"version":3,"names":["TIRUtils","formatDate","formatTime","name","data","levels","key","value","duration","color","desc","target","index","tirList","canvas","ctx","highTarget","lowTarget","highTargetHeight","lowTargetHeight","created","mounted","console","log","Date","$bus","$on","new_data","_","uniqBy","v_data","map","result","getTIRResult","$store","dispatch","round","Number","veryHighRate","highRate","normalRate","lowRate","veryLowRate","Math","floor","veryHighDuration","highDuration","normalDuration","lowDuration","veryLowDuration","filter","e","$nextTick","drawTir","state","report","tir","handelHeight","methods","legend","sortBy","$refs","offsetTop","document","getElementById","getContext","W","H","offsetHeight","width","height","scale","clearRect","barW","splitInterval","lineStart","lineWidth","start_y","start_text_y","noRangeLength","i","length","fillStyle","fillRect","tir_key_top","lineOneX","lineOneY","lineTwoX","lineTwoY","lineThreeX","lineThreeY","strokeStyle","beginPath","moveTo","lineTo","stroke","beforeDestroy","$off"],"sources":["src/views/components/Chart/TIR.vue"],"sourcesContent":["<template>\n    <!-- <div class='TIR' ref='tir'>\n        <canvas id='myCanvas' ref='canvas' class='canvas' ></canvas>\n    </div> -->\n    <div class='TIR' ref='tir'>\n        <canvas id='myCanvas' ref='canvas' class='canvas' ></canvas>\n        <div class='tir-info' >\n            <div class='tir-key' >\n                <div  v-for='(item) in tirList'  :key='item.key'>\n                    <div class='tir-key-item' ref='tirInfo'  :data-index='item.index'  v-if='item.value!=0'>\n                        <div class='tir-key-box'>\n                            <div class='tir-key-legend' :style='{backgroundColor:item.color}' ref='legend' ></div>\n                            <div class='tir-key-desc' >\n                                <div class='tir-key-key' >{{item.key}}</div>\n                                <div class='tir-key-range' >{{item.desc}}</div>\n                            </div>\n                        </div>\n                        <div class='tir-value'>\n                            <div class='tir-key-key ' > <span :class='[item.value>70?\"active\":\"\",\"tir-print-key\"]' >{{item.value}}%</span>({{item.duration[0]}}时{{item.duration[1]}}分)</div>\n                            <div class='tir-key-range' v-if='item.index==2'>目标＞{{item.target}}%</div>\n                            <div class='tir-key-range' v-else>目标＜{{item.target}}%</div>\n                        </div>\n                    </div>\n                </div>\n                <div class='tir-all-1' v-if='highTarget!=0' :style='{height:highTargetHeight+\"px\"}' >\n                    <div class='tir-all-line'></div>\n                    <div class='tir-all-value' >\n                        <div class='tir-key-key' >{{highTarget}}%</div>\n                        <div class='tir-key-range' >目标＜25%</div>\n                    </div>\n                </div>\n                <div class='tir-all-2'  v-if='lowTarget!=0' :style='{height:lowTargetHeight+\"px\"}'>\n                    <div class='tir-all-line' ></div>\n                    <div class='tir-all-value'  >\n                        <div class='tir-key-key' >{{lowTarget}}%</div>\n                        <div class='tir-key-range' >目标＜4%</div>\n                    </div>\n                </div>\n            </div>\n            \n        </div>\n    </div>\n</template>\n<script>\nimport { TIRUtils } from \"@/utils/algorithm/TIR\";\nimport {formatDate,formatTime} from '@/utils/formatTime'\nexport default {\n    name:'TIR',\n    data(){\n        return{\n            levels: [\n                {key: '2 级高血糖', value:1,  duration:[],color: '#E98C41', desc: '>13.9mmol/L',target:'5',index:0},\n                {\n                    key: '1 级高血糖',\n                    value: 1,\n                    duration:[],\n                    color: '#F6C059',\n                    desc: '5.1-13.9mmol/L',\n                    target:'25',\n                    index:1\n                },\n                {\n                    key: '目标范围内',\n                    value: 60,\n                    duration:[],\n                    color: '#32BAC0',\n                    desc: '3.9~10.0mmol/L',\n                    target:'70',\n                    index:2\n                },\n                {key: '1 级低血糖', value: 30,  duration:[],color: '#F43F31', desc: '3.0~3.8mmol/L',target:'4', index:3},\n                {key: '2 级低血糖', value:0,  duration:[],color: '#96251C', desc: '<3.0mmol/L',target:'1', index:4},\n            ],\n            tirList:[],\n            canvas:null,\n            ctx:null,\n            highTarget:0,\n            lowTarget:0,\n            highTargetHeight:0,\n            lowTargetHeight:0,\n        }\n    },\n    created(){\n    },\n    mounted(){\n        console.log(formatTime(new Date()),'组件渲染完成')\n        this.$bus.$on('getDatas',(data)=>{\n            let new_data = _.uniqBy(data[1],'DataTs')\n            let v_data = _.map(new_data,'Value')\n            let result = TIRUtils.getTIRResult(v_data)\n            console.log(result)\n            this.$store.dispatch('setTir',{key:data[0],value:result}) //处理完数据存vuex\n            this.levels[0].value = _.round(Number(result.veryHighRate*100),1)\n            this.levels[1].value = _.round(Number(result.highRate*100),1)\n            this.levels[2].value = _.round(Number(result.normalRate*100),1)\n            this.levels[3].value = _.round(Number(result.lowRate*100),1)\n            this.levels[4].value = _.round(Number(result.veryLowRate*100),1)\n            this.levels[0].duration =[Math.floor(result.veryHighDuration/60)<10 ? '0'+ Math.floor(result.veryHighDuration/60) : Math.floor(result.veryHighDuration/60), result.veryHighDuration%60 < 10 ? '0' + result.veryHighDuration%60 : result.veryHighDuration%60]\n            this.levels[1].duration =[Math.floor(result.highDuration/60)<10 ? '0'+ Math.floor(result.highDuration/60) : Math.floor(result.highDuration/60), result.highDuration%60 < 10 ? '0' + result.highDuration%60 : result.highDuration%60]\n            this.levels[2].duration =[Math.floor(result.normalDuration/60)<10 ? '0'+ Math.floor(result.normalDuration/60) : Math.floor(result.normalDuration/60), result.normalDuration%60 < 10 ? '0' + result.normalDuration%60 : result.normalDuration%60]\n            this.levels[3].duration =[Math.floor(result.lowDuration/60)<10 ? '0'+ Math.floor(result.lowDuration/60) : Math.floor(result.lowDuration/60), result.lowDuration%60 < 10 ? '0' + result.lowDuration%60 : result.lowDuration%60]\n            this.levels[4].duration =[Math.floor(result.veryLowDuration/60)<10 ? '0'+ Math.floor(result.veryLowDuration/60) : Math.floor(result.veryLowDuration/60), result.veryLowDuration%60 < 10 ? '0' + result.veryLowDuration%60 : result.veryLowDuration%60]\n       \n            this.highTarget = this.levels[0].value && this.levels[1].value ?_.round(this.levels[0].value+this.levels[1].value,1):0\n            this.lowTarget =  this.levels[3].value && this.levels[4].value ?_.round(this.levels[4].value+this.levels[4].value,1):0\n            this.tirList = _.filter(this.levels,function(e){return e.value>0})\n            this.$nextTick(()=>{\n                this.drawTir()\n            }) \n        })\n        this.$bus.$on('getVuexTag',(data)=>{\n            let result  = this.$store.state.report.tir[data]\n            console.log(result)\n             this.levels[0].value = _.round(Number(result.veryHighRate*100),1)\n            this.levels[1].value = _.round(Number(result.highRate*100),1)\n            this.levels[2].value = _.round(Number(result.normalRate*100),1)\n            this.levels[3].value = _.round(Number(result.lowRate*100),1)\n            this.levels[4].value = _.round(Number(result.veryLowRate*100),1)\n            console.log(result.veryHighDuration/60,result.veryHighDuration%60)\n            this.levels[0].duration =[Math.floor(result.veryHighDuration/60)<10 ? '0'+ Math.floor(result.veryHighDuration/60) : Math.floor(result.veryHighDuration/60), result.veryHighDuration%60 < 10 ? '0' + result.veryHighDuration%60 : result.veryHighDuration%60]\n            this.levels[1].duration =[Math.floor(result.highDuration/60)<10 ? '0'+ Math.floor(result.highDuration/60) : Math.floor(result.highDuration/60), result.highDuration%60 < 10 ? '0' + result.highDuration%60 : result.highDuration%60]\n            this.levels[2].duration =[Math.floor(result.normalDuration/60)<10 ? '0'+ Math.floor(result.normalDuration/60) : Math.floor(result.normalDuration/60), result.normalDuration%60 < 10 ? '0' + result.normalDuration%60 : result.normalDuration%60]\n            this.levels[3].duration =[Math.floor(result.lowDuration/60)<10 ? '0'+ Math.floor(result.lowDuration/60) : Math.floor(result.lowDuration/60), result.lowDuration%60 < 10 ? '0' + result.lowDuration%60 : result.lowDuration%60]\n            this.levels[4].duration =[Math.floor(result.veryLowDuration/60)<10 ? '0'+ Math.floor(result.veryLowDuration/60) : Math.floor(result.veryLowDuration/60), result.veryLowDuration%60 < 10 ? '0' + result.veryLowDuration%60 : result.veryLowDuration%60]\n            this.highTarget = this.levels[0].value && this.levels[1].value ?_.round(this.levels[0].value+this.levels[1].value,1):0\n            this.lowTarget =  this.levels[3].value && this.levels[4].value ?_.round(this.levels[4].value+this.levels[4].value,1):0\n            this.tirList = _.filter(this.levels,function(e){return e.value>0})\n            this.$nextTick(()=>{\n                this.drawTir()\n                this.handelHeight()\n            })\n           \n        })\n       \n    },\n    methods:{\n        // 计算高血糖，低血糖总比例的高度\n        handelHeight(){\n              let legend = _.sortBy(this.$refs.legend,['dataset.index']) //图例的位置\n            if(this.highTarget!=0){\n                // 计算高度\n                this.highTargetHeight = legend[1].offsetTop - legend[0].offsetTop\n            }\n            if(this.lowTarget!=0){\n                this.lowTargetHeight = legend[4].offsetTop - legend[3].offsetTop\n            }\n        },\n        // 画图\n        drawTir(){\n            const canvas = document.getElementById(\"myCanvas\");\n            const ctx = canvas.getContext(\"2d\")\n            const W = 160\n            const H = this.$refs.tir.offsetHeight*2\n            canvas.width = W\n            canvas.height = H\n            ctx.scale(2, 2)\n            ctx.clearRect(0, 0, canvas.width, canvas.height);\n            \n            const barW = 40 //柱状图宽度\n            const splitInterval = 1 //间隔距离\n            const lineStart = barW+5 //标识线x轴起点\n            const lineWidth =30//标识线总宽度\n    