{"ast":null,"code":"import ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      height: 1080\n    };\n  },\n  components: {\n    ScaleBox\n  },\n  created() {\n    this.height = window.innerHeight;\n  },\n  mounted() {\n    console.log(window.innerHeight);\n\n    // new devPixelRatio().init(); // 初始化页面比例\n  },\n  methods: {\n    beforeunloadHandler(e) {\n      if (this.$store.getters.upStep == 4) {\n        e = e || window.event;\n        if (e) {\n          e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n    },\n    pageScale() {\n      let browerWidth = window.innerWidth;\n      let browerHeight = window.innerHeight;\n      let baseWidth = 1920;\n      let zoom = browerWidth / baseWidth;\n      console.log(browerHeight);\n      document.documentElement.style.transform = \"scaleX(\" + zoom + \")\";\n      document.documentElement.style.height = browerHeight + 'px';\n      window.onresize = function () {\n        browerWidth = window.innerWidth;\n        zoom = browerWidth / baseWidth;\n        document.documentElement.style.transform = \"scaleX(\" + zoom + \")\";\n        document.documentElement.style.height = browerHeight + 'px';\n      };\n    }\n  },\n  destroyed() {\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n};","map":{"version":3,"names":["ScaleBox","devPixelRatio","name","data","height","components","created","window","innerHeight","mounted","console","log","methods","beforeunloadHandler","e","$store","getters","upStep","event","returnValue","pageScale","browerWidth","innerWidth","browerHeight","baseWidth","zoom","document","documentElement","style","transform","onresize","destroyed","removeEventListener"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <scale-box :width=\"1920\" :height=\"height\" bgc=\"transparent\" :delay=\"100\">\n     <router-view />\n    </scale-box>\n  </div>\n</template>\n\n<script>\nimport ScaleBox from \"vue2-scale-box\";\nimport devPixelRatio from \"@/utils/devicePixelRatio.js\";\nexport default {\n  name: 'App',\n  data(){\n    return{\n      height:1080\n    }\n  },\n  components: { ScaleBox },\n  created(){\n     this.height = window.innerHeight\n  },\n  mounted(){\n    console.log(window.innerHeight)\n   \n    // new devPixelRatio().init(); // 初始化页面比例\n  },\n  methods: {\n    beforeunloadHandler(e){\n      if(this.$store.getters.upStep == 4){\n       e = e || window.event;\n        if (e) {\n            e.returnValue = '关闭提示';\n        }\n        return '关闭提示';\n      }\n     \n    },\n    pageScale(){\n      let browerWidth = window.innerWidth\n      let browerHeight = window.innerHeight\n      let baseWidth = 1920\n      let zoom = browerWidth/baseWidth\n      console.log(browerHeight)\n     document.documentElement.style.transform= \"scaleX(\"+zoom+\")\"\n     document.documentElement.style.height=browerHeight+'px'\n      window.onresize = function(){\n        browerWidth = window.innerWidth\n        zoom = browerWidth/baseWidth\n        document.documentElement.style.transform= \"scaleX(\"+zoom+\")\"\n        document.documentElement.style.height=browerHeight+'px'\n      }\n    }\n  },\n  destroyed(){\n    window.removeEventListener('beforeunload', e => this.beforeunloadHandler(e));\n  }\n}\n</script>\n\n<style>\nhtml,body{\n  width:1920px !important;\n  height:100%;\n  padding:0;\n  margin:0;\n  transform-origin: 0 0;\n}\n#app {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  width:100%;\n  height:100%;\n}\n</style>\n"],"mappings":"AASA,OAAAA,QAAA;AACA,OAAAC,aAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;IACA;EACA;EACAC,UAAA;IAAAL;EAAA;EACAM,QAAA;IACA,KAAAF,MAAA,GAAAG,MAAA,CAAAC,WAAA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,CAAAJ,MAAA,CAAAC,WAAA;;IAEA;EACA;EACAI,OAAA;IACAC,oBAAAC,CAAA;MACA,SAAAC,MAAA,CAAAC,OAAA,CAAAC,MAAA;QACAH,CAAA,GAAAA,CAAA,IAA