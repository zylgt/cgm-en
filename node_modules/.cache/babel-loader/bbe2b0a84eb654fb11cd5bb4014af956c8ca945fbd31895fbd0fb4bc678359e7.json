button  @click=\"addSetting = false\">取消</el-button>\n                </div>\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport {getWeb} from '@/api/api'\nexport default {\n    data(){\n        return{\n            formData:{\n                order:'',\n                item_name:'',\n                config_key:'diabetes_type'\n            },\n            formRules:{\n                order: [{ required: true, trigger: \"blur\", message: \"请填写顺序号\" }],\n                item_name: [{ required: true, trigger: \"blur\", message: \" 请填写配置项名称\" }]\n            },\n            loading:false, //添加配置项loading\n            listLoading:false,\n            list:'',\n            addSetting:false,  //弹窗是否显示\n            dialogTitle:'', //弹窗标题\n        }\n    },\n    created(){\n\n    },\n\n    methods:{\n        // 获取网页协议\n        getList(){\n            getWeb(this.formData).then(response => {\n                        this.loading=false\n                        console.log(response)\n                        if(response.code == 1000){\n                            \n                        }else{\n                            this.$message({\n                                type: 'error',\n                                message: response.msg\n                            });\n                        }\n                }).catch((res) => {\n                        this.$message({\n                            type: 'error',\n                            message: res.message\n                        });\n                })\n        },\n    }\n}\n</script>\n<style scoped>\n    .filter-container{\n        margin-bottom:20px;\n    }\n    .record-btn{\n        display: flex;\n        justify-content: flex-end;\n    }\n</style>"],"mappings":"AAiDA,SAAAA,MAAA;AACA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,KAAA;QACAC,SAAA;QACAC,UAAA;MACA;MACAC,SAAA;QACAH,KAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,SAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,OAAA;MAAA;M