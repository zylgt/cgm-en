              this.loading = true\n                        updata(this.formData).then(response => {\n                            this.loading=false\n\t\t\t\t\t\t\tif(response.code == 1000){\n\t\t\t\t\t\t\t\tthis.addType = false\n                                this.$message({\n                                    message: '修改成功',\n                                    type: 'success'\n                                });\n                                this.getInfos(this.config_key)\n\t\t\t\t\t\t\t}else{\n\t\t\t\t\t\t\t\tthis.$message({\n                                    type: 'error',\n                                    message: response.msg\n\t\t\t\t\t\t\t\t});\n\t\t\t\t\t\t\t}\n                        }).catch((res) => {\n                                this.$message({\n                                    type: 'error',\n                                    message: res.message\n                                });\n                        })\n                    }\n               })\n        },\n        // 切换类型\n        typeClick(tab){\n            this.getInfos(tab.name)\n        },\n        // 新增弹窗\n        addDialog(){\n            this.addType = true\n            this.dialogTitle = '添加配置项'\n            this.editType = 1\n            this.formData = {\n                item_name:'',\n                config_key:this.config_key\n            }\n        },\n        // 修改弹窗\n        edit(info){\n            this.addType = true\n            this.dialogTitle = '修改配置项'\n            this.editType = 2\n            this.formData = {\n                id:info.id,\n                item_name:info.item_name,\n                is_active:info.is_active\n            }\n        },\n        // 排序\n        changeSort(config_key,arr){\n            let that = this\n            sort({config_key:config_key,orders:arr}).then(response => {\n                if(response.code == 1000){\n\n                }else{\n                    that.getInfos(that.config_key)\n                    that.$message({\n                        type: 'error',\n                        message: response.msg\n                    });\n                }\n            }).catch((res) => {\n                that.getInfos(that.config_key)\n                    that.$message({\n                        type: 'error',\n                        message: res.message\n                    });\n            })\n        }\n    }\n}\n</script>\n<style scoped>\n    .filter-container{\n        margin-bottom:20px;\n    }\n    .record-btn{\n        display: flex;\n        justify-content: flex-end;\n    }\n</style>"],"mappings":";AAsDA,SAAAA,MAAA,EAAAC,QAAA,EAAAC,MAAA,EAAAC,UAAA,EAAAC,IAAA;AACA,OAAAC,UAAA;AACA,SAAAC,UAAA;AACA,OAAAC,QAAA;AACA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,SAAA;MACA;MACAC,SAAA;QACAC,KAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,SAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,OAAA;MAAA;MACAC,WAAA;MACAC,IAAA;MACAf,UAAA;MACAgB,OAAA;MAAA;MACAC,WAAA;MAAA;MACAC,QAAA;MAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,aAAA;EACA;EACAC,QAAA;IACA,MAAAC,EAAA,GAAAC,QAAA,CAAAC,aAAA;IACA,IAAAC,IAAA;IACAtB,QAAA,CAAAP,MAAA,CAAA0B,EAAA;MACAI,SAAA;MACAC,UAAA;MACAC,KAAA,WAAAA,CAAA,WAAAC,GAAA;QACA,IAAAC,GAAA;QACA,IAAAC,OAAA,GAAAN,IAAA,CAAAX,IAAA,CAAAkB,MAAA,CAAAH,GAAA,CAAAI,QAAA;QACAR,IAAA,CAAAX,IAAA,CAAAkB,MAAA,CAAAH,GAAA,CAAAK,QAAA,KAAAH,OAAA;QACAN,IAAA,CAAAX,IAAA,CAAAqB,OAAA,CAAAC,IAAA;UACAN,GAAA,CAAAO,IAAA,CAAAD,IAAA,CAAAE,QAAA;QACA;QACAb,IAAA,CAAAc,UAAA,CAAAd,IAAA,CAAAP,UAAA,EAAAY,GAAA;MACA;IACA;EACA;EACAU,OAAA;IACAC,WAAAC,GAAA;MACA,OAAAxC,UAAA,KAAAyC,IAAA,CAAAD,GAAA,CAAAE,SAAA;IACA;IACAC,aAAAH,GAAA;MACA,OAAAxC,UAAA,KAAAyC,IAAA,CAAAD,GAAA,CAAAI,SAAA;IACA;IACA;IACA1B,cAAA;MACArB,UAAA,GAAAgD,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAC,IAAA;UACA,KAAAlD,UAAA,GAAAiD,QAAA,CAAA5C,IAAA;UACA,KAAA8C,QAAA,CAAAF,QAAA,CAAA5C,IAAA,IAAAc,UAAA;QACA;UACA,KAAAiC,QAAA;YACAC,IAAA;YACAzC,OAAA,EAAAqC,QAAA,CAAAK;UACA;QACA;MACA,GAAAC,KAAA,CAAAC,GAAA;QACA,KAAAJ,QAAA;UACAC,IAAA;UACAzC,OAAA,EAAA4C,GAAA,CAAA5C;QACA;MACA;IACA;IAC